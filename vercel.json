{
    "version": 2,
    "buildCommand": "cd client && npm install && npm run build",
    "outputDirectory": "client/.next",
    "installCommand": "cd client && npm install",
    "framework": "nextjs",
    "builds": [
        {
            "src": "client/package.json",
            "use": "@vercel/next"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/client/$1"
        }
    ]
}