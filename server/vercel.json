{
    "version": 2,
    "functions": {
        "index.js": {
            "maxDuration": 30
        }
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "index.js"
        },
        {
            "src": "/webhook/(.*)",
            "dest": "index.js"
        },
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ],
    "crons": [
        {
            "path": "/api/cron/cleanup",
            "schedule": "0 0 * * *"
        }
    ]
}
